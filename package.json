{
  "private": true,
  "sideEffects": false,
  "name": "hack_tales",
  "description": "A Slack Application built using Slack's Bolt SDK for the Node.js Runtime. Meant for posting blogs anonymously in Slack channels, though a system that is smooth, clean, private with a log of logging and records. This was made as an private self hosted alliterative to HackClub's Scrapbook.",
  "version": "0.0.1",
  "license": "GPL-3.0",
  "main": "src/app.js",
  "packageManager": "npm@10.9.0",
  "author": {
    "name": "An Anonymous HackClubber",
    "url": "https://https://hackclub.com"
  },
  "engines": {
    "node": "=>22.12.0",
    "npm": "=>10.9.0"
  },
  "cpu": [
    "arm",
    "arm64",
    "ia32",
    "mips",
    "mipsel",
    "ppc",
    "ppc64",
    "s390",
    "s390x",
    "x64"
  ],
  "os": [
    "!aix",
    "!android",
    "darwin",
    "!freebsd",
    "linux",
    "!openbsd",
    "!sunos",
    "win32"
  ],
  "keywords": [
    "Node.js, HackClub, HackClub Scrapbook, Hackathon, Programming, Blog, Diary, JavaScript, NPM, Slack Bot, Slack SDK, Slack API, Slack Bolt"
  ],
  "contributors": [
    {
      "name": "Boo"
    }
  ],
  "scripts": {
    "start:web": "node application/source/web/server.js",
    "dev:web": "nodemon application/source/web/server.js",
    "dev": "nodemon application/source/bot",
    "start-windows": "cls && application/source/bot/app.js",
    "dev:app:windows": "nodemon application/source/bot/app.js",
    "start-linux": "clear && node application/source/bot/app.js",
    "dev:app:linux": "nodemon application/source/bot/app.js",
    "start-mac": "clear && node application/source/bot/app.js",
    "dev:app:mac": "nodemon application/source/bot/app.js"
  },
  "config": {
    "BlahajGood": "true"
  },
  "dependencies": {
    "@slack/bolt": "^4.2.0",
    "@slack/oauth": "^3.0.2",
    "@slack/web-api": "^7.8.0",
    "concurrently": "^9.1.2",
    "dotenv": "^16.4.7",
    "ejs": "^3.1.10",
    "express": "^4.21.2"
  },
  "devDependencies": {
    "nodemon": "^3.1.9"
  }
}
